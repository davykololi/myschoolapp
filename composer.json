{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "akaunting/laravel-apexcharts": "^2.0",
        "arielmejiadev/larapex-charts": "^2.1",
        "artesaos/seotools": "*",
        "barryvdh/laravel-dompdf": "*",
        "blade-ui-kit/blade-icons": "*",
        "blade-ui-kit/blade-zondicons": "*",
        "codeat3/blade-academicons": "*",
        "codeat3/blade-carbon-icons": "*",
        "cviebrock/eloquent-sluggable": "*",
        "cybercog/laravel-ban": "*",
        "davidhsianturi/blade-bootstrap-icons": "*",
        "ellgreen/laravel-loadfile": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "hi-folks/statistics": "^0.2.1",
        "illuminate/support": "*",
        "intervention/image": "^2.7",
        "koolreport/laravel": "^2.0",
        "lab404/laravel-impersonate": "*",
        "laravel/framework": "^12.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "*",
        "maatwebsite/excel": "*",
        "mikehaertl/phpwkhtmltopdf": "^2.5",
        "milon/barcode": "*",
        "php-flasher/flasher-laravel": "*",
        "phpoffice/phpspreadsheet": "^1.29",
        "picqer/php-barcode-generator": "^2.3",
        "royceltd/laravel-bulksms": "^1.5",
        "simplesoftwareio/simple-qrcode": "*",
        "spatie/laravel-google-calendar": "^3.6",
        "spatie/laravel-permission": "*",
        "spatie/laravel-searchable": "*",
        "technohive-kenya/laravel-bulk-sms": "dev-master",
        "yajra/laravel-datatables-oracle": "*",
        "yazan/laravel-settings": "^1.0"
    },
    "require-dev": {
        "laravel/breeze": "*",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files":[
            "app/Helpers/GradingHelpers/gradinghelpers.php",
            "app/Helpers/MarksHelpers/markshelpers.php",
            "app/Helpers/PaymentHelpers/Payment.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
